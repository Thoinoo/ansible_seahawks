[nester_db_slave]
nester_db_2 ansible_host=10.1.0.72 id=2 mysql_replication_role=slave
nester_db_3 ansible_host=10.1.0.73 id=3 mysql_replication_role=slave

[orchestrator]
mysql-orchestrator ansible_host=10.1.0.49

